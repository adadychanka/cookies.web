{
    "env": {
        "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "plugin:import/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "linebreak-style": ["error","unix"],
        "react/prop-types": 0
    }
}
